generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
  engineType = "client"
}

datasource db {
  provider = "postgresql"
}

model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  name  String
  url Url []
}

model Url {
  id Int @id @default(autoincrement())
  slug String @unique
  originalUrl String
  user User @relation(fields: [userId], references: [id])
  userId Int
  clickCount Int
  createdAt DateTime
}